<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
</div>
<%= @user.name %>, <%= @user.email %>
<h3> Date Created:  </h3>
<%= @user.created_at %>
<h3> Date Updated: </h3>
<%= @user.updated_at %>
 <h3> Time Now: </h3>
<%= Time.now %>
